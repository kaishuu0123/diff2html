<div id="{{fileHtmlId}}" class="d2h-file-wrapper" data-lang="{{file.language}}">
    <div class="d2h-file-header">
      {{{filePath}}}
    </div>
    <div class="d2h-files-diff" style="overflow-x: auto; overflow-y: hidden;">
        <div class="d2h-file-side-diff-with-table">
            <div class="d2h-code-wrapper">
                <table class="d2h-diff-table d2h-diff-table-fixed">
                    <colgroup>
                      <col class="d2h-code-side-with-table-linenumber">
                      <col>
                      <col class="d2h-code-side-with-table-linenumber">
                      <col>
                    </colgroup>
                    <tbody class="d2h-diff-tbody-with-table">
                      {{{diffs.header}}}
                      {{#diffs.lines}}
                      <tr>
                        {{{left}}}
                        {{{right}}}
                      </tr>
                      {{/diffs.lines}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
